<Application x:Class="WeChatCrawler.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:WeChatCrawler"
             StartupUri="MainWindow.xaml"
             DispatcherUnhandledException="Application_DispatcherUnhandledException">
  <Application.Resources>
    <ObjectDataProvider x:Key="MyVendors" ObjectType="{x:Type local:VendorCollection}" />
    <ObjectDataProvider x:Key="Posts" ObjectType="{x:Type local:PostCollection}" />
  </Application.Resources>
</Application>
